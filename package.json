{
  "name": "inkskill-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "yarn dev-server",
    "windev": "yarn windev-server",
    "dev-server": "BABEL_ENV=development webpack-dev-server --inline --config webpack/development.js",
    "windev-server": "SET BABEL_ENV=development&&webpack-dev-server --inline --config webpack/development.js",
    "clean": "rm -rf ./dist",
    "build": "BABEL_ENV=production npm run clean && webpack -p --progress --config webpack/production.js",
    "build-stag": "BABEL_ENV=production npm run clean && webpack -p --progress --config webpack/staging.js",
    "precommit": "yarn flow && yarn lint",
    "lint": "LIST=`git diff-index --name-only HEAD | grep .*\\.js | grep -v json`; if [ \"$LIST\" ]; then eslint $LIST; fi",
    "lint-all": "eslint --ext .jsx --ext .js ./src/**",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "mocha --compilers js:babel-register --recursive src/**/*.test.js",
    "deploy": "yarn install && yarn build",
    "deploy-stag": "yarn install && yarn build-stag"
  },
  "keywords": [],
  "author": "InkSkill LLC <contact@inkskill.com> (http://inkskill.com)",
  "license": "MIT",
